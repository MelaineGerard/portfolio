{% extends 'base.html.twig' %}

{% block body %}
    <div class="container mx-auto">
        <div class="grid grid-cols-4 gap-4">
            {% for project in projects %}
                {{ component('project', {
                    projectName: project.name,
                    projectDescription: project.description,
                    projectLink: project.link,
                }) }}
            {% endfor %}
        </div>
    </div>
{% endblock %}
